public class Solution {
    public int XorOperation(int n, int start) {
        
        int[] nums = new int[n];

        for(int i =0; i<n; i++)
        {
            nums[i] = start + 2 * i;
        }

        int k = 0;
        foreach(int i in nums)
        {
            k = k ^ i;
        }

        return k;
    }
}
